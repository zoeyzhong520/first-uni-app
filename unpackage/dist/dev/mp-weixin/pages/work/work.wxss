












































/*头部标题*/
.header {
	background-color: rgb(239,239,239);
	height: 30px;
}
.header-title {
	font-size: 15px;
	color: rgb(120,122,123);
	padding-left: 10px;
	line-height: 30px;
}
/*菜单View*/
.itemsView {
	background-color: #FFFFFF;
	height: 160px;
}
/*菜单标题*/
.itemsTitle {
	font-size: 15px;
	display: block;
	padding: 10px;/*设置内边距 */
}
/*菜单详情View*/
.itemsDetailView {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
}
/*子菜单*/
.item {
	width: 33.3%;
	height: 120px;
	text-align: center;
}
/*子菜单图片*/
.itemImg {
	display: block;
	width: 60px;
	height: 60px;
	margin: 10px auto;
}
/*子菜单标题*/
.itemTitle {
	display: block;
	font-size: 15px;
	line-height: 16px;
}


